    <DotNetCliToolReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Tools" Version="2.0.4" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resourses\SharedResource.ru.Designer.cs">
      <DesignTime>True</DesignTime>
